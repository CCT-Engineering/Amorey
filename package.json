{
  "name": "atlier",
  "version": "1.0.0",
  "description": "Online shopping portal",
  "main": "client/src/index.js",
  "scripts": {
    "start": "npm run client-dev & npm run server-dev & live-server --open=./client/dist/index.html; fg",
    "from-server": "npm run client-dev & npm run server-dev & open http://localhost:3300; fg",
    "client-dev": "./node_modules/.bin/webpack --config ./webpack.config.js -w",
    "server-dev": "nodemon server/index.js",
    "test": "jest --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "deploy": "webpack --config ./webpack.config.js; screen -d -m node server/index.js",
    "show-screens": "screen -ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CCT-Engineering/Atelier.git"
  },
  "author": "CCT",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CCT-Engineering/Atelier/issues"
  },
  "homepage": "https://github.com/CCT-Engineering/Atelier#readme",
  "dependencies": {
    "axios": "^1.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "identity-obj-proxy": "3.0.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serve-favicon": "^2.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.2",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "live-server": "^1.2.2",
    "style-loader": "^3.3.1",
